        var publicationByYearDataUrlformat = 'http://academic.research.microsoft.com/io.ashx?type=3&entitytype=8&id=434&year=';
        var publicationByYearUrlformat = 'http://academic.research.microsoft.com/Detail?entitytype=8&searchtype=2&id=434';
