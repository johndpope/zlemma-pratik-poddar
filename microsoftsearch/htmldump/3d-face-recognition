                        var isSilverlightInstalled = false;
                        function CheckSilverlightInstalled() {
                            try {
                                try {
                                    var slControl = new ActiveXObject('AgControl.AgControl'); //IE
                                    isSilverlightInstalled = true;
                                }
                                catch (e) {
                                    if (navigator.plugins["Silverlight Plug-In"]) //non-IE
                                    {
                                        isSilverlightInstalled = true;
                                    }
                                }
                            }
                            catch (e) { }
                        }
