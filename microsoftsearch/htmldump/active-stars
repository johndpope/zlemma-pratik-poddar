    <script type="text/javascript">
        var publicationByYearDataUrlformat = 'http://academic.research.microsoft.com/io.ashx?type=3&entitytype=8&id=444&year=';
        var publicationByYearUrlformat = 'http://academic.research.microsoft.com/Detail?entitytype=8&searchtype=2&id=444';
        function ShowListByYear(year, type) {
            if (type == "publication") {
                showPublicationsByYear(SiteRoot, publicationByYearDataUrlformat + year, "Publication in " + year, "divPaperList");
                $("#hypListTitle").css("color", "#00bfe7");
                $("#hypListTitle").attr("href", publicationByYearUrlformat + "&year=" + year);
            }
            else {
                $("#divPaperList").hide();
            }
        }                                 
    </script>
