    //<![CDATA[
    var redirectUrlctl00_MainContent_PaperList_OrderByFilter = 'http://academic.research.microsoft.com/Detail?entitytype=8&searchtype=2&id=104';
    $(function () {
        //author
        var data = { 'inputId': '#ctl00_MainContent_PaperList_OrderByFilter_drpList_drpList',
            "hiddenId": '#ctl00_MainContent_PaperList_OrderByFilter_drpList_drp_hidden_focus',
