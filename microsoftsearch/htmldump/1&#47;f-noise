<script type="text/javascript" language="javascript">
    //<![CDATA[
    var redirectUrlctl00_MainContent_PaperList_OrderByFilter = 'http://academic.research.microsoft.com/Detail?entitytype=8&searchtype=2&id=4';
    $(function () {
        //author
        var data = { 'inputId': '#ctl00_MainContent_PaperList_OrderByFilter_drpList_drpList',
            "hiddenId": '#ctl00_MainContent_PaperList_OrderByFilter_drpList_drp_hidden_focus',
            'textId': '#ctl00_MainContent_PaperList_OrderByFilter_drpList_spSelectedText',
            'tsId': '#ctl00_MainContent_PaperList_OrderByFilter_drpList_drpListContent',
            'postAction': function () {   
                var value = $(DropDown.tsId + ' .selected').eq(0).find(':hidden').text();
                var url = window.location.href;             
                
                var regExp = /&orderBy=\d+/ig;
                url = url.replace(regExp, "");                
                var origin_value = url.match(RegExp);
                if(value > 0 && typeof origin_value != "undefined" && jQuery.trim(origin_value) == jQuery.trim(value)){
                    return;
                }

                regExp = /&start=\d+/ig;
                url = url.replace(regExp, "");
                regExp = /\?start=\d+/ig;
                url = url.replace(regExp, "?");
                regExp = /&end=\d+/ig;
                url = url.replace(regExp, "");
                regExp = /\?end=\d+/ig;
                url = url.replace(regExp, "?");

                if(redirectUrlctl00_MainContent_PaperList_OrderByFilter != '')
                {
                    //do not remain in the same page
                    url = redirectUrlctl00_MainContent_PaperList_OrderByFilter;
                }

                if (value > 0) {
                    url = url + "&orderBy=" + value;
                    window.location = url;
                }
                else {
                    window.location = url;
                }
            }
        };
        DropDown.Init(data);
        var options = new Array();

        options[0] = { "Text": "Year", "Value": "-1", "Selected": "true" };options[1] = { "Text": "Citation", "Value": "1" };options[2] = { "Text": "Rank", "Value": "2" };

        DropDown.SetOptions(options);
        var startYear = GetStartYear();
        if (startYear && startYear != "") {
            DropDown.SetDefaultValue(startYear);
        }
    })

    function GetStartYear() {
        var lastYearValue = decodeURI(request("orderBy"));
        return lastYearValue;
    }
    //]]>
</script>
    
