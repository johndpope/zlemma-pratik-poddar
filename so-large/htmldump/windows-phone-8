           data-gps-track="posts_hot_network.click({ item_type:3, location:9 })">
            more hot questions
        </a>
        <script>
            StackExchange.ready(function () {
                var $div = $('#hot-network-questions'),
                    $more = $div.find('.js-show-more'),
                    $hidden = $div.find('.js-hidden');

                $more.click(function () {
                    $hidden.show();
                    $more.remove();
                    return false;
                });

            
            });
        </script>
