n=50
ratio1 &lt;- seq(0,1.5,(1.5-0)/(n-1))
ratio2 &lt;- seq(0,2.5,(2.5-0)/(n-1))
ratio3 &lt;- seq(0.5,4.5,(4.5-0.5)/(n-1))
...
